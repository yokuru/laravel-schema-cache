{
    "name": "yokuru/laravel-schema-cache",
    "description": "Cache the schema manager",
    "keywords": [
        "laravel",
        "doctrine",
        "schema",
        "database",
        "cache"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "yokuru",
            "email": "takashi.yokubo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php" : "^7.1",
        "illuminate/support": "^5.8",
        "illuminate/console": "^5.8",
        "illuminate/database": "^5.8",
        "illuminate/filesystem": "^5.8",
        "doctrine/dbal": "^2.10",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Yokuru\\SchemaCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yokuru\\SchemaCacheTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yokuru\\SchemaCache\\ServiceProvider"
            ],
            "aliases": {
                "SchemaCache": "Yokuru\\SchemaCache\\Facade\\SchemaCache"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.5"
    }
}
